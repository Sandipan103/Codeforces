# include <bits/stdc++.h>
using namespace std;
 
int main()  {
    int t;
    cin >> t;
    while(t--)  {
        string s, match = "314159265358979323846264338327";
        cin >> s;
        int cnt = 0;
        for(int i=0; i<s.length(); i++) {
            if(s[i] != match[i])    break;
            else cnt++;
        }
        cout << cnt++ << endl;
    }
    return 0;
}
