#include<iostream>
using namespace std;
 
int main()  {
    int n,count = 0;
    cin >> n;
    for(int i=0; i<n; i++)  {
        int arr[3];
        int num = 0;
        for(int j=0; j<3; j++)  {
            
            cin >> arr[j];
            if(arr[j] == 1)
            num++;
        }
        if(num >= 2)
        count++;
    }
    cout << count;
    return 0;
}
